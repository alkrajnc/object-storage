{
    "compilerOptions": {
        "module": "CommonJS",
        "esModuleInterop": true,
        "target": "ES2017",
        "noImplicitAny": true,
        "moduleResolution": "Node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "*": [
                "node_modules/*",
            ],
            "@db/*": [
                "src/db/*"
            ],

        }
    },
    "include": [
        "src/**/*"
    ]
}
